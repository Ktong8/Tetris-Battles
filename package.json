{
    "name": "tetris",
    "version": "1.0.0",
    "scripts": {
        "client": "tsc && cd client && yarn start",
        "server": "tsc && nodemon dist/server.js",
        "dev": "tsc && concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
    },
    "dependencies": {
        "@types/express": "^4.17.13",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "http-status-codes": "^2.1.4"
    },
    "devDependencies": {
        "concurrently": "^4.0.1"
    }
}
